<p id="notice"><%= notice %></p>

<h1>Account Listings</h1>

<div class="row">
  <% @listings.each do |listing| %>
  <div class="col-md-6">
    <div class="panel panel-default">
      <div class="panel-heading">
        <%= listing account_owner %>
      </div>
      <div class="panel-body">





<table>
  <thead>
    <tr>
      <th>Account owner</th>
      <th>Account title</th>
      <th>Account number</th>
      <th>Account balance</th>
      <th>Web address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.account_owner %></td>
        <td><%= listing.account_title %></td>
        <td><%= listing.account_number %></td>
        <td><%= listing.account_balance %></td>
        <td><%= listing.web_address %></td>
        <td><%= link_to 'Show', listing %></td>
        <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
        <td><%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing', new_listing_path %>
